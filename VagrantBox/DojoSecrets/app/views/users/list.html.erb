<div class="container">
    <h1 class="display-4">All Users</h1>
    <table>
        <thead>
            <th>User ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th># Secrets</th>
            <th># Liked Secrets</th>
        </thead>
        <% for user in @users %>
            <tr>
                <td><%= user.id %></td>
                <td><%= user.first_name %></td>
                <td><%= user.last_name %></td>
                <td><%= user.email %></td>
                <td><%= user.secrets.count %></td>
                <td><%= user.secrets_liked.count %></td>
            </tr>
        <% end %>
    </table>
</div>